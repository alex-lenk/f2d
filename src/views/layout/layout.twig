{% block content %}{% endblock %}

{% block footer %}{% endblock %}

{% block modal %}{% endblock %}

{% block search %}{% endblock %}

{% block entertainment %}{% endblock %}

{% block searchPanel %}{% endblock %}

{% block searchResult %}{% endblock %}

{% block mobileMenu %}{% endblock %}

{% block scripts %}
  {% include '../partials/scripts.twig' %}
  <div class="darkening-layer"></div>
{% endblock %}
